{
  "name": "tsbot",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/src/app.js",
  "repository": {
    "url": "git+ssh://git@github.com:inmyownwords7/tsbot.git"
  },
  "scripts": {
    "build:tsup": "tsup src/app.ts --format esm --dts --clean",
    "build": "npm run build:tsup",
    "start": "npm run build:tsup && node -r tsconfig-paths/register dist/app.js",
    "start:node": "npm run build && node --no-deprecation dist/src/app.js tsc --traceResolution",
    "debug": "npm run build && node --no-deprecation --inspect dist/src/app.js",
    "inspect": "npm run build && node --inspect dist/src/app.js",
    "docs": "typedoc --out docs src/bot.ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/collections": "^5.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.3",
    "@types/xregexp": "^4.3.0",
    "esbuild-plugin-alias": "^0.2.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typedoc": "^0.26.7",
    "typedoc-umlclass": "^0.10.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@twurple/api": "^7.2.0",
    "@twurple/auth": "^7.2.0",
    "@twurple/chat": "^7.2.0",
    "@twurple/eventsub-base": "^7.2.0",
    "@twurple/eventsub-ws": "^7.2.0",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "collections": "^5.1.13",
    "dotenv": "^16.4.5",
    "dotenv-vault": "^1.26.2",
    "express": "^4.21.0",
    "module-alias": "^2.2.3",
    "promise-fs": "^2.1.1",
    "strip-ansi": "^7.1.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "xregexp": "^5.1.1"
  }
}
